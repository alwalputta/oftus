<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.myapp.util.Feedback" table="Feedback">
        <id column="feedback_id" name="feedbackId" type="string" length="200">
            <generator class="uuid"/>
        </id>
        <property name="name" column="name" type="string"></property>
        <property name="email" column="email" type="string"></property>
        <property name="phone" column="phone" type="string"></property>
        <property name="country" column="country" type="string"></property>
        <property name="issueType" column="issue_type" type="string"></property>
        <property name="description" column="description" type="string"></property>
        <property name="createDate" column="create_date" type="string"></property>
    </class>
</hibernate-mapping>
