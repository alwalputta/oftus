<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.myapp.main.BookmarkClick" table="BookmarkClick">
        <id column="bookmark_click_id" name="bookmarkClickId" type="int">
            <generator class="increment"/>
        </id>
        <property name="bookmarkId" column="bookmark_id" type="int"></property>
        <property name="hiperLink" column="hiper_link" type="string"></property>
        <property name="createDate" column="create_date" type="string"></property>
        <property name="lastModifiedDate" column="last_modified_date" type="string"></property>
    </class>
</hibernate-mapping>
